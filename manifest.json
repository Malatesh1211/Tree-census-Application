{
   "name": "Tree Census App",
   "short_name": "TreeCensus",
   "id": "/tree-census",
   "description": "A progressive web application for managing tree census data.",
   "start_url": "malatesh1211.github.io/Tree-census-Application/",
   "display": "standalone",
   "display_override": ["window-controls-overlay"],
   "background_color": "#ffffff",
   "theme_color": "#4CAF50",
   "orientation": "portrait-primary",
   "launch_handler": {
       "client_mode": "navigate-existing"
   },
   "edge_side_panel": {
       "preferred_width": 400
   },
   "file_handlers": {
       "files": [
           {
               "action": "/open-file",
               "accept": {
                   "application/json": [".json"],
                   "text/csv": [".csv"]
               }
           }
       ]
   },
   "handle_links": "preferred",
   "protocol_handlers": [
       {
           "protocol": "web+treecensus",
           "url": "/?data=%s"
       }
   ],
   "share_target": {
       "action": "/share",
       "method": "POST",
       "enctype": "multipart/form-data",
       "params": {
           "title": "title",
           "text": "text",
           "url": "url"
       }
   },
   "shortcuts": [
       {
           "name": "Add Tree",
           "short_name": "Add",
           "description": "Add a new tree to the census",
           "url": "/add-tree",
           "icons": [{ "src": "icon-add.png", "sizes": "192x192", "type": "image/png" }]
       }
   ],
   "categories": ["utilities", "productivity"],
   "dir": "ltr",
   "iarc_rating_id": "e12345",
   "lang": "en-US",
   "prefer_related_applications": false,
   "related_applications": [
       {
           "platform": "play",
           "url": "https://play.google.com/store/apps/details?id=com.treecensus",
           "id": "com.treecensus"
       }
   ],
   "scope": "/",
   "scope_extensions": [
       {
           "origin": "https://example.com"
       }
   ]
}
